{
  "name": "@layerzerolabs/lz-evm-sdk-v2",
  "version": "3.0.59",
  "license": "LZBL-1.2",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./artifacts/*": "./artifacts/*",
    "./artifacts-tron/*": "./artifacts-tron/*",
    "./artifacts-zk/*": "./artifacts-zk/*",
    "./deployments/*": "./deployments/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "artifacts/contracts/**/*",
    "artifacts-tron/contracts/**/*",
    "artifacts-zk/contracts/**/*",
    "deployments/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "$npm_execpath clean-prebuild && $npm_execpath populate && $npm_execpath generate-typechain-types && $npm_execpath build-ts",
    "build-ts": "$npm_execpath tsc --noEmit && $npm_execpath tsup",
    "clean": "rimraf dist src/typechain-types .turbo",
    "clean-prebuild": "rimraf -g dist src/typechain-types artifacts 'src/errors/*.json'",
    "generate-typechain-types": "$npm_execpath typechain --target ethers-v5 'artifacts/contracts/**/!(*.dbg).json' --out-dir src/typechain-types",
    "prepack": "rsync --delete -ac ./deployments/ ./.backup/ && ./scripts/trim.sh ./deployments 'address,transactionHash,abi'",
    "postpack": "rsync --delete -ac ./.backup/ ./deployments/ && rm -rf ./.backup/",
    "populate": "$npm_execpath ts-node scripts/populate.ts"
  },
  "dependencies": {
    "@layerzerolabs/evm-sdks-core": "^3.0.59",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@layerzerolabs/evm-sdks-build": "^3.0.59",
    "@layerzerolabs/lz-definitions": "^3.0.59",
    "@layerzerolabs/lz-evm-messagelib-v2": "^3.0.59",
    "@layerzerolabs/lz-evm-oapp-v2": "^3.0.59",
    "@layerzerolabs/lz-evm-protocol-v2": "^3.0.59",
    "@layerzerolabs/monorepo-utilities": "^0.0.0",
    "@layerzerolabs/tsup-config-next": "^3.0.59",
    "@layerzerolabs/typescript-config-next": "^3.0.59",
    "@openzeppelin/contracts": "^4.8.1",
    "@typechain/ethers-v5": "^10.2.1",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.10.5",
    "hardhat-deploy": "^0.12.4",
    "rimraf": "^5.0.5",
    "solidity-bytes-utils": "^0.8.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.3.5",
    "typechain": "^8.3.2",
    "typescript": "~5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "lzVersions": {
    "default": [
      "v2"
    ],
    "v1": [
      "Executor",
      "Executor_Implementation",
      "Executor_Proxy",
      "ExecutorFeeLib",
      "ExecutorProxyAdmin",
      "PriceFeed",
      "PriceFeed_Implementation",
      "PriceFeed_Proxy",
      "PriceFeedProxyAdmin",
      "DVNFeeLib",
      "DVN",
      "SendUln301",
      "ReceiveUln301",
      "Treasury"
    ],
    "v2": [
      "Executor",
      "Executor_Implementation",
      "Executor_Proxy",
      "ExecutorFeeLib",
      "ExecutorProxyAdmin",
      "PriceFeed",
      "PriceFeed_Implementation",
      "PriceFeed_Proxy",
      "PriceFeedProxyAdmin",
      "DVNFeeLib",
      "DVN",
      "SendUln302",
      "ReceiveUln302",
      "Treasury"
    ]
  }
}
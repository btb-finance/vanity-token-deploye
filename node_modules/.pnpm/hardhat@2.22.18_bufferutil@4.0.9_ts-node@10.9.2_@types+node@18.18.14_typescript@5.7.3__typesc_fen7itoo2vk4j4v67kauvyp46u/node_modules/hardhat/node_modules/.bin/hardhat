else
  export NODE_PATH="/Users/macos/mylayerzero2/node_modules/.pnpm/hardhat@2.22.18_bufferutil@4.0.9_ts-node@10.9.2_@types+node@18.18.14_typescript@5.7.3__typesc_fen7itoo2vk4j4v67kauvyp46u/node_modules/hardhat/internal/cli/node_modules:/Users/macos/mylayerzero2/node_modules/.pnpm/hardhat@2.22.18_bufferutil@4.0.9_ts-node@10.9.2_@types+node@18.18.14_typescript@5.7.3__typesc_fen7itoo2vk4j4v67kauvyp46u/node_modules/hardhat/internal/node_modules:/Users/macos/mylayerzero2/node_modules/.pnpm/hardhat@2.22.18_bufferutil@4.0.9_ts-node@10.9.2_@types+node@18.18.14_typescript@5.7.3__typesc_fen7itoo2vk4j4v67kauvyp46u/node_modules/hardhat/node_modules:/Users/macos/mylayerzero2/node_modules/.pnpm/hardhat@2.22.18_bufferutil@4.0.9_ts-node@10.9.2_@types+node@18.18.14_typescript@5.7.3__typesc_fen7itoo2vk4j4v67kauvyp46u/node_modules:/Users/macos/mylayerzero2/node_modules/.pnpm/node_modules:$NODE_PATH"

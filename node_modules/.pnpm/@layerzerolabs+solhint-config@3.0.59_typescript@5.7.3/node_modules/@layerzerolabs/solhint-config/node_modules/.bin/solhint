#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macos/mylayerzero2/node_modules/.pnpm/solhint@4.5.4_typescript@5.7.3/node_modules/solhint/node_modules:/Users/macos/mylayerzero2/node_modules/.pnpm/solhint@4.5.4_typescript@5.7.3/node_modules:/Users/macos/mylayerzero2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macos/mylayerzero2/node_modules/.pnpm/solhint@4.5.4_typescript@5.7.3/node_modules/solhint/node_modules:/Users/macos/mylayerzero2/node_modules/.pnpm/solhint@4.5.4_typescript@5.7.3/node_modules:/Users/macos/mylayerzero2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../solhint@4.5.4_typescript@5.7.3/node_modules/solhint/solhint.js" "$@"
else
  exec node  "$basedir/../../../../../../solhint@4.5.4_typescript@5.7.3/node_modules/solhint/solhint.js" "$@"
fi
